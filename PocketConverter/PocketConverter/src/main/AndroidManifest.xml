        android:icon="@mipmap/ico_launcher"
        android:label="Pocket Converter"
        android:supportsRtl="true"
        android:theme="@android:style/Theme.Holo" >
        <activity
            android:name=".MainWindow"
            android:label="Pocket Converter"
            android:theme="@style/AppTheme.NoActionBar" >
